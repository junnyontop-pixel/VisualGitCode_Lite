{
  "manifest_version": 3,
  "name": "GitHub Web Editor Enhancer",
  "version": "0.0.1",
  "description": "Make GitHub web editor feel like a real IDE",

  "permissions": ["scripting"],
  "host_permissions": [
    "https://github.com/*"
  ],

  "content_scripts": [
    {
      "matches": ["https://github.com/*"],
      "js": ["content.js"],
      "css": ["style.css"]
    }
  ]
}